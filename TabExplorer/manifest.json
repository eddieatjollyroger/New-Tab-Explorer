{
  "manifest_version": 2,
  "name": "Tab Explorer",
  "version": "3",
    "background": {
  "scripts": ["js/background.js"]
},
  "description": "New tab page showing tabs as a file explorer",
  "permissions": [
    "tabs",
    "topSites",
    "search",
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  }
}
